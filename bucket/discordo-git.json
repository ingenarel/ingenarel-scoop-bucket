{
    "version": "1769fa97e1789b16257b6d5dbcae4d2ef2c1800e",
    "description": "A lightweight, secure, and feature-rich Discord terminal client (git https clone)",
    "license": "MIT license",
    "homepage": "https://github.com/ayn2op/discordo",
    "notes": "Type discordo to start",
    "url": "https://raw.githubusercontent.com/ingenarel/ingenarel-scoop-bucket/refs/heads/master/decoy",
    "hash": "159bf09babfa63914e51fc50a4eabdf2d80c638768021961138e2c477ea66f79",
    "depends": [
        "go",
        "git"
    ],
    "pre_install": [
        "echo '' 'trying to uninstall conflicts'",
        "scoop uninstall discordo discordo-git-ssh",
        "cd $dir",
        "git clone --depth 1 --branch main https://github.com/ayn2op/discordo.git",
        "echo 'building discordo...' ''",
        "cd discordo",
        "go build -ldflags '-s -w' .",
        "echo 'Removing unneeded stuff' ''",
        "cd $",
        "Move-Item discordo/discordo.exe discordo.exe",
        "Remove-Item -Recurse -Force discordo",
        "Remove-Item decoy"
    ],
    "bin": [
        "discordo.exe"
    ],
    "conflicts": [
        "discordo",
        "discordo-git-ssh"
    ]
}
