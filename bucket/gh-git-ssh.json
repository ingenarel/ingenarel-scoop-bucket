{
    "version": "dae074049a6ccdf2863172dc30ff9f459351f545",
    "description": "GitHub\u2019s official command line tool (git version uses ssh cloning)",
    "license": "MIT license",
    "homepage": "https://github.com/cli/cli",
    "notes": "This is the git version.\nConficts with main bucket's discordo, so i remove it in the start.",
    "url": "https://raw.githubusercontent.com/ingenarel/ingenarel-scoop-bucket/refs/heads/master/decoy",
    "hash": "159bf09babfa63914e51fc50a4eabdf2d80c638768021961138e2c477ea66f79",
    "depends": [
        "go",
        "git"
    ],
    "pre_install": [
        "echo '' 'trying to uninstall conflicts'",
        "scoop uninstall gh gh-git",
        "cd ~/scoop/apps/gh-git-ssh",
        "git clone --depth 1 --branch trunk git@github.com:cli/cli.git",
        "cd cli",
        "echo 'building gh...'",
        "go run script\\build.go bin\\gh.exe GO_LDFLAGS='-s -w'"
    ],
    "bin": [
        "bin/gh.exe"
    ],
    "conflicts": [
        "gh",
        "gh-git"
    ]
}
