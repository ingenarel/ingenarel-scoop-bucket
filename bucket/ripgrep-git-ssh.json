{
    "version": "79cbe89deb1151e703f4d91b19af9cdcc128b765",
    "description": "ripgrep recursively searches directories for a regex pattern while respecting your gitignore (git version uses https clone)",
    "license": "MIT license",
    "homepage": "https://github.com/BurntSushi/ripgrep",
    "notes": "This is the git version.\nUses https clone.",
    "url": "https://raw.githubusercontent.com/ingenarel/ingenarel-scoop-bucket/refs/heads/master/decoy",
    "hash": "159bf09babfa63914e51fc50a4eabdf2d80c638768021961138e2c477ea66f79",
    "depends": [
        "rustup-msvc",
        "git"
    ],
    "pre_install": [
        "echo '' 'trying to uninstall conflicts'",
        "scoop uninstall ripgrep ripgrep-git",
        "cd ~/scoop/apps/ripgrep-git-ssh",
        "git clone --depth 1 --branch master git@github.com:BurntSushi/ripgrep.git",
        "echo 'building ripgrep...' ''",
        "cd ripgrep",
        "cargo build --release --locked"
    ],
    "bin": [
        "target/release/rg.exe"
    ],
    "conflicts": [
        "ripgrep",
        "ripgrep-git"
    ]
}
