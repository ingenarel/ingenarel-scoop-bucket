    "homepage": "https://github.com/junegunn/fzf",
    "notes": "This is the git version.\nUses https clone.",
    "url": "https://raw.githubusercontent.com/ingenarel/ingenarel-scoop-bucket/refs/heads/master/decoy",
    "hash": "159bf09babfa63914e51fc50a4eabdf2d80c638768021961138e2c477ea66f79",
    "depends": [
        "go",
        "git"
    ],
    "pre_install": [
        "echo '' 'trying to uninstall conflicts'",
        "scoop uninstall fzf fzf-git-ssh",
        "cd $dir",
        "git clone --depth 1 --branch master https://github.com/junegunn/fzf.git",
        "echo 'building fzf...' ''",
        "cd fzf",
        "go build -ldflags '-s -w' ."
    ],
    "bin": [
        "fzf.exe"
    ],
    "conflicts": [
        "fzf",
        "fzf-git-ssh"
    ]
}
