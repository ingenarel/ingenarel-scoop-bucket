{
    "version": "d98a9c0ff48eaaf7621cf7426dd4ac387f5bf7ff",
    "description": "A GitLab CLI tool bringing GitLab to your command line (git version uses ssh clone)",
    "license": "MIT license",
    "homepage": "https://gitlab.com/gitlab-org/cli",
    "notes": "This is the git version.\nUses ssh clone\nTo not authenticate via your glab token everytime, set an enviroment variable called \"GITLAB_TOKEN\" and set it to your gitlab token.",
    "url": "https://raw.githubusercontent.com/ingenarel/ingenarel-scoop-bucket/refs/heads/master/decoy",
    "hash": "159bf09babfa63914e51fc50a4eabdf2d80c638768021961138e2c477ea66f79",
    "depends": [
        "git",
        "go",
        "make"
    ],
    "pre_install": [
        "echo '' 'trying to uninstall conflicts'",
        "scoop uninstall glab glab-git",
        "cd ~/scoop/apps/glab-git-ssh",
        "git clone --depth 1 --branch main git@gitlab.com:gitlab-org/cli.git",
        "cd cli",
        "make build",
        "cd bin",
        "Move-Item glab glab.exe"
    ],
    "bin": [
        "glab.exe"
    ],
    "conflicts": [
        "glab",
        "glab-git"
    ]
}
