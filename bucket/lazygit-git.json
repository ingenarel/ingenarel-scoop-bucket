{
    "version": "d11e11d179ec7df1b14a536a3965b254430b0504",
    "description": "Simple terminal UI for git commands (git version uses https cloning)",
    "license": "MIT license",
    "homepage": "https://github.com/jesseduffield/lazygit",
    "notes": "This is the git version.\nUses https cloning",
    "url": "https://raw.githubusercontent.com/ingenarel/ingenarel-scoop-bucket/refs/heads/master/decoy",
    "hash": "159bf09babfa63914e51fc50a4eabdf2d80c638768021961138e2c477ea66f79",
    "depends": [
        "go",
        "git"
    ],
    "pre_install": [
        "echo '' 'trying to uninstall conflicts'",
        "scoop uninstall lazygit lazygit-git-ssh",
        "cd $dir",
        "git clone --depth 1 --branch master https://github.com/jesseduffield/lazygit.git",
        "echo 'building lazygit...' ''",
        "cd lazygit",
        "go build -ldflags '-s -w' ."
    ],
    "bin": [
        "lazygit.exe"
    ],
    "conflicts":[
        "lazygit",
        "lazygit-git-ssh"
    ]
}
