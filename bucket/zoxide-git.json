{
    "version": "d5f12fcceaae2e8df635c347c18a1b1f8a9e6467",
    "description": "A smarter cd command. Supports all major shells. (git version uses https cloning)",
    "license": "MIT license",
    "homepage": "https://github.com/ajeetdsouza/zoxide",
    "notes": "This is the git version.\nUses https cloning.\nGo to https://github.com/shunsambongi/clink-zoxide if you want to see how to set up the z and zi aliases in cmd, clink is also needed for that\nAnd fzf is also needed to use the zi command",
    "url": "https://raw.githubusercontent.com/ingenarel/ingenarel-scoop-bucket/refs/heads/master/decoy",
    "hash": "159bf09babfa63914e51fc50a4eabdf2d80c638768021961138e2c477ea66f79",
    "depends": [
        "rustup-msvc",
        "git"
    ],
    "pre_install": [
        "echo '' 'trying to uninstall conflicts'",
        "scoop uninstall zoxide zoxide-git-ssh",
        "cd ~/scoop/apps/zoxide-git",
        "git clone --depth 1 --branch master https://github.com/ajeetdsouza/zoxide.git",
        "echo 'building zoxide...' ''",
        "cd zoxide",
        "cargo build --release --locked"
    ],
    "bin": [
        "target/release/zoxide.exe"
    ],
    "conflicts": [
        "zoxide",
        "zoxide-git"
    ]
}
